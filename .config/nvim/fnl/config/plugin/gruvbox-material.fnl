(module config.plugin.gruvbox-material)

(set vim.g.gruvbox_material_background "hard") ; Although I use Neovide with transparency, Conjure's HUD benefits from this.
(set vim.g.gruvbox_material_enable_italic true)
(set vim.g.gruvbox_material_transparent_background 2)
(set vim.g.gruvbox_material_ui_contrast "low")
(set vim.g.gruvbox_material_diagnostic_virtual_text "colored")
(set vim.g.gruvbox_material_better_performance true)


